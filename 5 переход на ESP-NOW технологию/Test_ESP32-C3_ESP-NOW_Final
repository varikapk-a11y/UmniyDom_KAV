// ======================================================================
// –ù–ê–ó–í–ê–ù–ò–ï: Test_ESP32-C3_ESP-NOW_Final.ino (–¥–ª—è ESP32 Arduino Core 3.3.5+)
// –£–°–¢–†–û–ô–°–¢–í–û: ESP32-C3 (—É–∑–µ–ª —Å MAC AC:EB:E6:49:10:28)
// –§–£–ù–ö–¶–ò–Ø: 1. –ü–æ–ª—É—á–∞–µ—Ç –∫–æ–º–∞–Ω–¥—ã LED_ON/LED_OFF –ø–æ ESP-NOW –æ—Ç —Ö–∞–±–∞
//          2. –í–∫–ª—é—á–∞–µ—Ç/–≤—ã–∫–ª—é—á–∞–µ—Ç —Å–≤–µ—Ç–æ–¥–∏–æ–¥ –Ω–∞ GPIO8
//          3. –û—Ç–ø—Ä–∞–≤–ª—è–µ—Ç –ø–æ–¥—Ç–≤–µ—Ä–∂–¥–µ–Ω–∏–µ ACK –æ–±—Ä–∞—Ç–Ω–æ —Ö–∞–±—É
// –ö–û–ú–ú–ï–ù–¢–ê–†–ò–ò: –í—Å–µ –≤–∞–∂–Ω—ã–µ —Å—Ç—Ä–æ–∫–∏ –ø—Ä–æ–∫–æ–º–º–µ–Ω—Ç–∏—Ä–æ–≤–∞–Ω—ã –¥–ª—è –ø–æ–Ω–∏–º–∞–Ω–∏—è –ª–æ–≥–∏–∫–∏.
// ======================================================================

// –ü–û–î–ö–õ–Æ–ß–ï–ù–ò–ï –ù–ï–û–ë–•–û–î–ò–ú–´–• –ë–ò–ë–õ–ò–û–¢–ï–ö
#include <WiFi.h>       // –ë–∏–±–ª–∏–æ—Ç–µ–∫–∞ –¥–ª—è —Ä–∞–±–æ—Ç—ã —Å Wi-Fi (–Ω—É–∂–Ω–∞ –¥–ª—è ESP-NOW)
#include <esp_now.h>    // –ë–∏–±–ª–∏–æ—Ç–µ–∫–∞ –ø—Ä–æ—Ç–æ–∫–æ–ª–∞ ESP-NOW

// –ö–û–ù–°–¢–ê–ù–¢–´ –ò –ù–ê–°–¢–†–û–ô–ö–ò –£–ó–õ–ê
#define NODE_ID 101       // –£–Ω–∏–∫–∞–ª—å–Ω—ã–π –∏–¥–µ–Ω—Ç–∏—Ñ–∏–∫–∞—Ç–æ—Ä —ç—Ç–æ–≥–æ —É–∑–ª–∞ –≤ —Å–µ—Ç–∏
#define LED_PIN 8         // –ü–∏–Ω, –∫ –∫–æ—Ç–æ—Ä–æ–º—É –ø–æ–¥–∫–ª—é—á–µ–Ω —Å–≤–µ—Ç–æ–¥–∏–æ–¥ (–Ω–∞ ESP32-C3 –≤—Å—Ç—Ä–æ–µ–Ω–Ω—ã–π LED –Ω–∞ GPIO8, –∞–∫—Ç–∏–≤–Ω—ã–π LOW)

// –û–ü–†–ï–î–ï–õ–ï–ù–ò–ï –°–¢–†–£–ö–¢–£–†–´ –î–ê–ù–ù–´–• –î–õ–Ø ESP-NOW
// –í–∞–∂–Ω–æ: —ç—Ç–∞ —Å—Ç—Ä—É–∫—Ç—É—Ä–∞ –¥–æ–ª–∂–Ω–∞ –±—ã—Ç—å –¢–û–ß–ù–û —Ç–∞–∫–æ–π –∂–µ –Ω–∞ —Ö–∞–±–µ –∏ –Ω–∞ –≤—Å–µ—Ö —É–∑–ª–∞—Ö!
typedef struct esp_now_message {
    char command[16];    // –¢–µ–∫—Å—Ç –∫–æ–º–∞–Ω–¥—ã –∏–ª–∏ –ø–æ–¥—Ç–≤–µ—Ä–∂–¥–µ–Ω–∏—è: "LED_ON", "LED_OFF", "ACK_ON", "ACK_OFF"
    uint8_t sender_id;   // ID –æ—Ç–ø—Ä–∞–≤–∏—Ç–µ–ª—è (–Ω–∞–ø—Ä–∏–º–µ—Ä, 1 –¥–ª—è —Ö–∞–±–∞, 101 –¥–ª—è —ç—Ç–æ–≥–æ —É–∑–ª–∞)
} esp_now_message;

// –°–û–ó–î–ê–ù–ò–ï –ì–õ–û–ë–ê–õ–¨–ù–´–• –ü–ï–†–ï–ú–ï–ù–ù–´–•
esp_now_message incomingMessage;  // –ë—É—Ñ–µ—Ä –¥–ª—è –≤—Ö–æ–¥—è—â–∏—Ö —Å–æ–æ–±—â–µ–Ω–∏–π
esp_now_message outgoingMessage;  // –ë—É—Ñ–µ—Ä –¥–ª—è –∏—Å—Ö–æ–¥—è—â–∏—Ö —Å–æ–æ–±—â–µ–Ω–∏–π

// MAC-–ê–î–†–ï–° –•–ê–ë–ê. –ó–ê–ú–ï–ù–ò–¢–ï –ù–ê –†–ï–ê–õ–¨–ù–´–ô –ê–î–†–ï–° –í–ê–®–ï–ì–û –•–ê–ë–ê!
// –§–æ—Ä–º–∞—Ç: {0x9C, 0x9C, 0x1F, 0xC7, 0x2D, 0x94} –¥–ª—è –∞–¥—Ä–µ—Å–∞ 9C:9C:1F:C7:2D:94
uint8_t hubMacAddress[] = {0x9C, 0x9C, 0x1F, 0xC7, 0x2D, 0x94};

// –ü–†–û–¢–û–¢–ò–ü–´ –§–£–ù–ö–¶–ò–ô (–æ–±—ä—è–≤–ª–µ–Ω–∏–µ —Ñ—É–Ω–∫—Ü–∏–π, –∫–æ—Ç–æ—Ä—ã–µ –±—É–¥—É—Ç –æ–ø–∏—Å–∞–Ω—ã –Ω–∏–∂–µ)
void onEspNowDataRecv(const esp_now_recv_info_t *recv_info, const uint8_t *data, int len);
void onEspNowDataSent(const wifi_tx_info_t *info, esp_now_send_status_t status);
void sendAckToHub(const char* ackCommand);

// ===================== –ù–ê–ß–ê–õ–¨–ù–ê–Ø –ù–ê–°–¢–†–û–ô–ö–ê (SETUP) =====================
void setup() {
    // 1. –ò–ù–ò–¶–ò–ê–õ–ò–ó–ê–¶–ò–Ø –ü–û–°–õ–ï–î–û–í–ê–¢–ï–õ–¨–ù–û–ì–û –ü–û–†–¢–ê –î–õ–Ø –û–¢–õ–ê–î–ö–ò
    Serial.begin(115200);
    // –ö–†–ò–¢–ò–ß–ï–°–ö–ê–Ø –ó–ê–î–ï–†–ñ–ö–ê –¥–ª—è ESP32-C3 —Å USB-CDC.
    // –î–∞—ë—Ç –≤—Ä–µ–º—è –æ–ø–µ—Ä–∞—Ü–∏–æ–Ω–Ω–æ–π —Å–∏—Å—Ç–µ–º–µ –ø–µ—Ä–µ–ø–æ–¥–∫–ª—é—á–∏—Ç—å—Å—è –∫ COM-–ø–æ—Ä—Ç—É –ø–æ—Å–ª–µ –ø–µ—Ä–µ–∑–∞–≥—Ä—É–∑–∫–∏ –ø–ª–∞—Ç—ã.
    delay(5000);
    
    Serial.println("\n=== –£–ó–ï–õ ESP-NOW (MAC: AC:EB:E6:49:10:28) ===");
    
    // 2. –ù–ê–°–¢–†–û–ô–ö–ê –°–í–ï–¢–û–î–ò–û–î–ê
    pinMode(LED_PIN, OUTPUT);
    digitalWrite(LED_PIN, HIGH); // –£—Å—Ç–∞–Ω–æ–≤–∫–∞ HIGH –¥–ª—è –≤—ã–∫–ª—é—á–µ–Ω–∏—è —Å–≤–µ—Ç–æ–¥–∏–æ–¥–∞ (–∞–∫—Ç–∏–≤–Ω—ã–π LOW)
    Serial.println("–°–≤–µ—Ç–æ–¥–∏–æ–¥ –≥–æ—Ç–æ–≤. –û–∂–∏–¥–∞–Ω–∏–µ –∫–æ–º–∞–Ω–¥...");
    Serial.print("MAC —ç—Ç–æ–≥–æ —É–∑–ª–∞: ");
    Serial.println(WiFi.macAddress()); // –í—ã–≤–æ–¥ —Å–æ–±—Å—Ç–≤–µ–Ω–Ω–æ–≥–æ MAC-–∞–¥—Ä–µ—Å–∞ –¥–ª—è —Å–ø—Ä–∞–≤–∫–∏
    
    // 3. –ù–ê–°–¢–†–û–ô–ö–ê –†–ï–ñ–ò–ú–ê WI-FI –ò –ú–û–©–ù–û–°–¢–ò –ü–ï–†–ï–î–ê–¢–ß–ò–ö–ê
    WiFi.mode(WIFI_STA); // –£—Å—Ç–∞–Ω–æ–≤–∫–∞ —Ä–µ–∂–∏–º–∞ Wi-Fi —Å—Ç–∞–Ω—Ü–∏–∏ (–æ–±—è–∑–∞—Ç–µ–ª—å–Ω–æ –¥–ª—è ESP-NOW)
    
    // –†–£–ß–ù–ê–Ø –ù–ê–°–¢–†–û–ô–ö–ê –ú–û–©–ù–û–°–¢–ò –ü–ï–†–ï–î–ê–¢–ß–ò–ö–ê –î–õ–Ø –°–ù–ò–ñ–ï–ù–ò–Ø –ù–ê–ì–†–ï–í–ê
    // –î–æ—Å—Ç—É–ø–Ω—ã–µ —É—Ä–æ–≤–Ω–∏ –º–æ—â–Ω–æ—Å—Ç–∏ (–≤ –ø–æ—Ä—è–¥–∫–µ —É–≤–µ–ª–∏—á–µ–Ω–∏—è):
    // WIFI_POWER_8_5dBm  (~7 –º–í—Ç)  - –º–∏–Ω–∏–º–∞–ª—å–Ω—ã–π –Ω–∞–≥—Ä–µ–≤, –º–∏–Ω–∏–º–∞–ª—å–Ω–∞—è –¥–∞–ª—å–Ω–æ—Å—Ç—å
    // WIFI_POWER_11dBm   (~13 –º–í—Ç)
    // WIFI_POWER_13dBm   (~20 –º–í—Ç)
    // WIFI_POWER_15dBm   (~32 –º–í—Ç)
    // WIFI_POWER_17dBm   (~50 –º–í—Ç)
    // WIFI_POWER_19_5dBm (~90 –º–í—Ç) - –∑–Ω–∞—á–µ–Ω–∏–µ –ø–æ —É–º–æ–ª—á–∞–Ω–∏—é, —Å–∏–ª—å–Ω—ã–π –Ω–∞–≥—Ä–µ–≤
    // WIFI_POWER_20dBm   (~100 –º–í—Ç)
    WiFi.setTxPower(WIFI_POWER_8_5dBm); // –£—Å—Ç–∞–Ω–æ–≤–∫–∞ –º–∏–Ω–∏–º–∞–ª—å–Ω–æ–π –º–æ—â–Ω–æ—Å—Ç–∏ –¥–ª—è —Å–Ω–∏–∂–µ–Ω–∏—è –Ω–∞–≥—Ä–µ–≤–∞
    
    // 4. –ò–ù–ò–¶–ò–ê–õ–ò–ó–ê–¶–ò–Ø ESP-NOW
    if (esp_now_init() != ESP_OK) {
        Serial.println("‚ùå –û—à–∏–±–∫–∞ –∏–Ω–∏—Ü–∏–∞–ª–∏–∑–∞—Ü–∏–∏ ESP-NOW!");
        while(1) delay(1000); // –ë–µ—Å–∫–æ–Ω–µ—á–Ω—ã–π —Ü–∏–∫–ª –ø—Ä–∏ –æ—à–∏–±–∫–µ
    }
    
    // 5. –†–ï–ì–ò–°–¢–†–ê–¶–ò–Ø –§–£–ù–ö–¶–ò–ô-–û–ë–†–ê–ë–û–¢–ß–ò–ö–û–í (–ö–û–õ–ë–≠–ö–û–í) ESP-NOW
    esp_now_register_recv_cb(onEspNowDataRecv); // –§—É–Ω–∫—Ü–∏—è –±—É–¥–µ—Ç –≤—ã–∑–≤–∞–Ω–∞ –ø—Ä–∏ –ø–æ–ª—É—á–µ–Ω–∏–∏ –¥–∞–Ω–Ω—ã—Ö
    esp_now_register_send_cb(onEspNowDataSent); // –§—É–Ω–∫—Ü–∏—è –±—É–¥–µ—Ç –≤—ã–∑–≤–∞–Ω–∞ –ø–æ—Å–ª–µ –æ—Ç–ø—Ä–∞–≤–∫–∏ –¥–∞–Ω–Ω—ã—Ö
    
    // 6. –î–û–ë–ê–í–õ–ï–ù–ò–ï –•–ê–ë–ê –í –°–ü–ò–°–û–ö "–ü–ò–†–û–í" (–£–°–¢–†–û–ô–°–¢–í) ESP-NOW
    // ESP-NOW –±—É–¥–µ—Ç –æ–±–º–µ–Ω–∏–≤–∞—Ç—å—Å—è –¥–∞–Ω–Ω—ã–º–∏ —Ç–æ–ª—å–∫–æ —Å –∑–∞—Ä–µ–≥–∏—Å—Ç—Ä–∏—Ä–æ–≤–∞–Ω–Ω—ã–º–∏ –ø–∏—Ä–∞–º–∏
    esp_now_peer_info_t peerInfo = {};          // –°–æ–∑–¥–∞–Ω–∏–µ –∏ –æ–±–Ω—É–ª–µ–Ω–∏–µ —Å—Ç—Ä—É–∫—Ç—É—Ä—ã
    memcpy(peerInfo.peer_addr, hubMacAddress, 6); // –ö–æ–ø–∏—Ä–æ–≤–∞–Ω–∏–µ MAC-–∞–¥—Ä–µ—Å–∞ —Ö–∞–±–∞
    peerInfo.channel = 0;                       // Wi-Fi –∫–∞–Ω–∞–ª (0 = —Ç–µ–∫—É—â–∏–π –∫–∞–Ω–∞–ª)
    peerInfo.encrypt = false;                   // –®–∏—Ñ—Ä–æ–≤–∞–Ω–∏–µ –æ—Ç–∫–ª—é—á–µ–Ω–æ –¥–ª—è –ø—Ä–æ—Å—Ç–æ—Ç—ã
    esp_now_add_peer(&peerInfo);                // –†–µ–≥–∏—Å—Ç—Ä–∞—Ü–∏—è —Ö–∞–±–∞ –∫–∞–∫ –ø–∏—Ä–∞
    
    Serial.println("‚úÖ –£–∑–µ–ª –≥–æ—Ç–æ–≤ –∫ —Ä–∞–±–æ—Ç–µ —Å —Ö–∞–±–æ–º.\n");
}

// ===================== –û–°–ù–û–í–ù–û–ô –¶–ò–ö–õ (LOOP) =====================
void loop() {
    // –û—Å–Ω–æ–≤–Ω–æ–π —Ü–∏–∫–ª –ø—É—Å—Ç–æ–π, –≤—Å—è –ª–æ–≥–∏–∫–∞ —Ä–∞–±–æ—Ç–∞–µ—Ç –≤ —Ñ—É–Ω–∫—Ü–∏—è—Ö-–æ–±—Ä–∞–±–æ—Ç—á–∏–∫–∞—Ö (–∫–æ–ª–±—ç–∫–∞—Ö)
    // –ø–æ —Å–æ–±—ã—Ç–∏—é –ø–æ–ª—É—á–µ–Ω–∏—è –¥–∞–Ω–Ω—ã—Ö. –ö–æ—Ä–æ—Ç–∫–∞—è –∑–∞–¥–µ—Ä–∂–∫–∞ –¥–ª—è —Å—Ç–∞–±–∏–ª—å–Ω–æ—Å—Ç–∏.
    delay(1000);
}

// ===================== –§–£–ù–ö–¶–ò–ò-–û–ë–†–ê–ë–û–¢–ß–ò–ö–ò (–ö–û–õ–ë–≠–ö–ò) =====================

// –§–£–ù–ö–¶–ò–Ø: –û–ë–†–ê–ë–û–¢–ö–ê –í–•–û–î–Ø–©–ò–• –î–ê–ù–ù–´–• –ü–û ESP-NOW
// –í—ã–∑—ã–≤–∞–µ—Ç—Å—è –∞–≤—Ç–æ–º–∞—Ç–∏—á–µ—Å–∫–∏ –ø—Ä–∏ –ø–æ–ª—É—á–µ–Ω–∏–∏ –ø–∞–∫–µ—Ç–∞ –ø–æ ESP-NOW
void onEspNowDataRecv(const esp_now_recv_info_t *recv_info, const uint8_t *incomingData, int len) {
    // –ò–∑–≤–ª–µ—á–µ–Ω–∏–µ MAC-–∞–¥—Ä–µ—Å–∞ –æ—Ç–ø—Ä–∞–≤–∏—Ç–µ–ª—è –∏–∑ —Å—Ç—Ä—É–∫—Ç—É—Ä—ã recv_info
    const uint8_t *senderMac = recv_info->src_addr;
    
    // –ö–æ–ø–∏—Ä–æ–≤–∞–Ω–∏–µ –ø–æ–ª—É—á–µ–Ω–Ω—ã—Ö –¥–∞–Ω–Ω—ã—Ö –≤ —Å—Ç—Ä—É–∫—Ç—É—Ä—É incomingMessage
    memcpy(&incomingMessage, incomingData, sizeof(incomingMessage));
    
    // –í–´–í–û–î –û–¢–õ–ê–î–û–ß–ù–û–ô –ò–ù–§–û–†–ú–ê–¶–ò–ò –í SERIAL
    Serial.print("üì• –ö–æ–º–∞–Ω–¥–∞ –æ—Ç —Ö–∞–±–∞ ");
    // –ö—Ä–∞—Å–∏–≤—ã–π –≤—ã–≤–æ–¥ MAC-–∞–¥—Ä–µ—Å–∞ –æ—Ç–ø—Ä–∞–≤–∏—Ç–µ–ª—è
    for(int i=0; i<6; i++) {
        Serial.print(senderMac[i], HEX);
        if(i < 5) Serial.print(":");
    }
    Serial.print(": ");
    Serial.print(incomingMessage.command);
    Serial.print(" (–æ—Ç ID:");
    Serial.print(incomingMessage.sender_id);
    Serial.println(")");
    
    // –û–ë–†–ê–ë–û–¢–ö–ê –ö–û–ù–ö–†–ï–¢–ù–´–• –ö–û–ú–ê–ù–î
    if(strcmp(incomingMessage.command, "LED_ON") == 0) {
        digitalWrite(LED_PIN, LOW);  // –í–∫–ª—é—á–µ–Ω–∏–µ —Å–≤–µ—Ç–æ–¥–∏–æ–¥–∞ (–∞–∫—Ç–∏–≤–Ω—ã–π LOW)
        Serial.println("üí° LED –í–ö–õ–Æ–ß–Å–ù.");
        sendAckToHub("ACK_ON");      // –û—Ç–ø—Ä–∞–≤–∫–∞ –ø–æ–¥—Ç–≤–µ—Ä–∂–¥–µ–Ω–∏—è –æ –≤–∫–ª—é—á–µ–Ω–∏–∏
    } else if(strcmp(incomingMessage.command, "LED_OFF") == 0) {
        digitalWrite(LED_PIN, HIGH); // –í—ã–∫–ª—é—á–µ–Ω–∏–µ —Å–≤–µ—Ç–æ–¥–∏–æ–¥–∞
        Serial.println("üí° LED –í–´–ö–õ–Æ–ß–ï–ù.");
        sendAckToHub("ACK_OFF");     // –û—Ç–ø—Ä–∞–≤–∫–∞ –ø–æ–¥—Ç–≤–µ—Ä–∂–¥–µ–Ω–∏—è –æ –≤—ã–∫–ª—é—á–µ–Ω–∏–∏
    }
    // –ó–¥–µ—Å—å –º–æ–∂–Ω–æ –¥–æ–±–∞–≤–∏—Ç—å –æ–±—Ä–∞–±–æ—Ç–∫—É –¥—Ä—É–≥–∏—Ö –∫–æ–º–∞–Ω–¥
}

// –§–£–ù–ö–¶–ò–Ø: –û–¢–ü–†–ê–í–ö–ê –ü–û–î–¢–í–ï–†–ñ–î–ï–ù–ò–Ø (ACK) –•–ê–ë–£
// –°–æ–∑–¥–∞—ë—Ç –∏ –æ—Ç–ø—Ä–∞–≤–ª—è–µ—Ç –ø–∞–∫–µ—Ç –ø–æ–¥—Ç–≤–µ—Ä–∂–¥–µ–Ω–∏—è –Ω–∞ —Ö–∞–±
void sendAckToHub(const char* ackCommand) {
    // –ó–∞–ø–æ–ª–Ω–µ–Ω–∏–µ —Å—Ç—Ä—É–∫—Ç—É—Ä—ã –∏—Å—Ö–æ–¥—è—â–µ–≥–æ —Å–æ–æ–±—â–µ–Ω–∏—è
    strncpy(outgoingMessage.command, ackCommand, sizeof(outgoingMessage.command)-1);
    outgoingMessage.sender_id = NODE_ID; // –£–∫–∞–∑–∞–Ω–∏–µ ID —ç—Ç–æ–≥–æ —É–∑–ª–∞
    
    // –û–¢–ü–†–ê–í–ö–ê –ü–ê–ö–ï–¢–ê –ü–û ESP-NOW
    esp_err_t result = esp_now_send(hubMacAddress, (uint8_t *) &outgoingMessage, sizeof(outgoingMessage));
    
    // –í–´–í–û–î –†–ï–ó–£–õ–¨–¢–ê–¢–ê –û–¢–ü–†–ê–í–ö–ò
    Serial.print("üì§ –û—Ç–ø—Ä–∞–≤–∫–∞ '");
    Serial.print(ackCommand);
    Serial.print("': ");
    Serial.println(result == ESP_OK ? "OK" : "ERROR");
}

// –§–£–ù–ö–¶–ò–Ø: –û–ë–†–ê–ë–û–¢–ö–ê –ü–û–î–¢–í–ï–†–ñ–î–ï–ù–ò–Ø –û–¢–ü–†–ê–í–ö–ò
// –í—ã–∑—ã–≤–∞–µ—Ç—Å—è –∞–≤—Ç–æ–º–∞—Ç–∏—á–µ—Å–∫–∏ –ø–æ—Å–ª–µ –ø–æ–ø—ã—Ç–∫–∏ –æ—Ç–ø—Ä–∞–≤–∫–∏ –ø–∞–∫–µ—Ç–∞ ESP-NOW
// –ü–∞—Ä–∞–º–µ—Ç—Ä 'info' —Å–æ–¥–µ—Ä–∂–∏—Ç —Ç–µ—Ö–Ω–∏—á–µ—Å–∫—É—é –∏–Ω—Ñ–æ—Ä–º–∞—Ü–∏—é –æ–± –æ—Ç–ø—Ä–∞–≤–∫–µ
// –ü–∞—Ä–∞–º–µ—Ç—Ä 'status' –ø–æ–∫–∞–∑—ã–≤–∞–µ—Ç —Ä–µ–∑—É–ª—å—Ç–∞—Ç: ESP_NOW_SEND_SUCCESS –∏–ª–∏ –æ—à–∏–±–∫–∞
void onEspNowDataSent(const wifi_tx_info_t *info, esp_now_send_status_t status) {
    if(status != ESP_NOW_SEND_SUCCESS) {
        Serial.println("‚ö†Ô∏è –ü–æ–¥—Ç–≤–µ—Ä–∂–¥–µ–Ω–∏–µ –Ω–µ –¥–æ—Å—Ç–∞–≤–ª–µ–Ω–æ.");
        // –í —Ä–µ–∞–ª—å–Ω–æ–º –ø—Ä–æ–µ–∫—Ç–µ –∑–¥–µ—Å—å –º–æ–∂–Ω–æ –¥–æ–±–∞–≤–∏—Ç—å –ª–æ–≥–∏–∫—É –ø–æ–≤—Ç–æ—Ä–Ω–æ–π –æ—Ç–ø—Ä–∞–≤–∫–∏
    }
}
